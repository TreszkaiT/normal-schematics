  "schematics": {
    "normal": {
      "description": "A blank schematic.",
      "factory": "./normal/index",
      "schema": "./normal/schema.json"
    },
    "normal-src": {
      "description": "Generate SRC files.",
      "factory": "./normal-src/index",
      "schema": "./normal-src/schema.json"
    },
    "normal-app": {
      "description": "Generate APP files.",
