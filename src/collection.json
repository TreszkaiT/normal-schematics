    "normal-module": {
      "description": "An normal module schematic.",
      "factory": "./normal-module/index",
      "schema": "./normal-module/schema.json"
    },
    "normal-page": {
      "description": "An normal Obj page schematic.",
      "factory": "./normal-page/index",
      "schema": "./normal-page/schema.json"
